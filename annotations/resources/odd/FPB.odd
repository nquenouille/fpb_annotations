<!--                <model behaviour="alternate" predicate="$parameters?mode='normalized'">-->
                    <model predicate="contains(@xml:id, 'a') and (concat('t', ./@xml:id) = ./following::add[@type='addition']/@xml:id) and not((concat('t', ./@xml:id) = ./following::add[@type='addition'][@hand = root($parameters?root)//scriptNote[@scribe='main']/@scribeRef]/@xml:id))" behaviour="inline">
                        <param name="name" value="'pb-popover'"/>
                        <param name="content" value="for $a in . return if(concat('t', $a/@xml:id) = $a/following::add/@xml:id) then $a/following::add[@xml:id = concat('t', $a/@xml:id)]/node() else ()"/>
                        <!-- CAVEAT: /db/apps/annotations/data/register.xml -->
                        <param name="alternate" value="if (following::add[1]/del[1]) then concat('Ergänzt von: ', for $a in . return if(concat('t', $a/@xml:id) = $a/following::add/@xml:id) then (if(starts-with($a/following::add[@xml:id = concat('t', $a/@xml:id)]/@hand, 'fpb-')) then $a/following::add[@xml:id = concat('t', $a/@xml:id)]/id(@hand, doc(&#34;/db/apps/annotations/data/register.xml&#34;))/persName/text() else if(not($a/following::add[@xml:id = concat('t', $a/@xml:id)]/@hand='')) then replace($a/following::add[@xml:id = concat('t', $a/@xml:id)]/@hand, 'involved', 'Unbekannter Schreiber')  else()) else (), ' Ursprüngliche Lesart: ', ./following::add[1]/string()) else concat('Ergänzt von: ', for $a in . return if(concat('t', $a/@xml:id) = $a/following::add/@xml:id) then (if(starts-with($a/following::add[@xml:id = concat('t', $a/@xml:id)]/@hand, 'fpb-')) then $a/following::add[@xml:id = concat('t', $a/@xml:id)]/id(@hand, doc(&#34;/db/apps/annotations/data/register.xml&#34;))/persName/text() else if(not($a/following::add[@xml:id = concat('t', $a/@xml:id)]/@hand='')) then replace($a/following::add[@xml:id = concat('t', $a/@xml:id)]/@hand, 'involved', 'Unbekannter Schreiber')  else()) else ()    )"/>
                        <param name="placement" value="'left'"/>
                        <param name="fallback-placement" value="'right'"/>
                        <pb:template xmlns="" xml:space="preserve"><pb-popover placement="[[placement]]" fallback-placement="[[fallback-placement]]">[[content]]<span slot="alternate">[[alternate]]</span></pb-popover></pb:template>
                        <outputRendition xml:space="preserve">
                            color: grey;
                        </outputRendition>
                    </model>
                    <model predicate="contains(@xml:id, 'a') and (concat('t', ./@xml:id) = ./following::add[@type='addition'][@hand = root($parameters?root)//scriptNote[@scribe='main']/@scribeRef]/@xml:id)" behaviour="inline">
                        <param name="name" value="'pb-popover'"/>
